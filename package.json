  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=4"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:node-gh/gh.git"
  },
  "bugs": {
    "url": "https://github.com/node-gh/gh/issues"
  },
  "keywords": [
    "git",
    "github",
    "external",
    "commands",
    "helpers"
  ],
  "bin": {
    "gh": "index.js"
  },
  "scripts": {
    "build": "gulp build",
    "ci": "gulp ci",
    "dev": "gulp watch",
    "format": "prettier-standard 'lib/*.js' && prettier-standard 'lib/**/*.js' && prettier-standard 'bin/*.js'",
    "precommit": "lint-staged",
    "postinstall": "npm run format",
    "test": "gulp test"
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "dependencies": {
    "inquirer": "^0.11.0",
    "moment": "^2.20.1",
    "opn": "^5.3.0",
    "update-notifier": "^0.5.0",
    "userhome": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^7.0.15",
    "gulp": "^3.9.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "lodash": "^4.17.10",
    "prettier": "^1.9.2",
    "prettier-standard": "^8.0.1",
    "tslint": "^5.10.0",
    "tslint-config-blvd": "^1.1.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  }
}
